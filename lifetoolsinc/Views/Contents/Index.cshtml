@model IEnumerable<lifetoolsinc.Models.Contents>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layoutassmt.cshtml";
}

<h2>Contens</h2>

<p>
    @Html.ActionLink("Add New Content", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>

    
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Preview", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
<div>
    <a href="@Url.Action("Index","Accounts")">Cancel</a>

</div>
